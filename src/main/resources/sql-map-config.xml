<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	
	<!-- Mabatis first 설정에 필요한 내용 -->
	
	<!-- Alias 설정 -->
	<typeAliases>
		
		<!-- alias="board" 라고 설정 -->
		<!-- mapping 파일에서는 resultType에 board라고 사용 -->
		<typeAlias alias="lantrip" type="com.tripfull.biz.lantrip.LanTripVO"/>
		
	</typeAliases>	
	
	<!-- Sql Mapper 설정 -->
	<mappers>
	
		<!-- 매핑파일 설정 -->
		<mapper resource="mappings/lantrip-mapping.xml"/>
		
	</mappers>
	
	
	
</configuration>